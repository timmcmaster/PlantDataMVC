@model ListViewModelStatic<PlantDataMVC.Web.Models.ViewModels.ProductType.ProductTypeListViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Product Types</h2>
<p>This is a list of all Product Types</p>

@if (PlantDataMvcConstants.UseBasicMvcViews)
{
    <a asp-action="New">Add New Item</a>
}

@await Component.InvokeAsync("ProductTypeGrid", new { model = Model })

@if (PlantDataMvcConstants.UseBasicMvcViews)
{
    @Html.PagingLinksFor(m => Model)
}

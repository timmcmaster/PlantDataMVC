@model ListViewModelStatic<PlantDataMVC.Web.Models.ViewModels.Plant.PlantListViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>My Plant List</h2>
<p>This is a list of all plant descriptions</p>


@if (PlantDataMvcConstants.UseBasicMvcViews)
{
    <a asp-action="New">Add New Item</a>
}

@await Component.InvokeAsync("PlantGrid", new { model = Model })

@if (PlantDataMvcConstants.UseBasicMvcViews)
{
    @Html.PagingLinksFor(m => Model)
}

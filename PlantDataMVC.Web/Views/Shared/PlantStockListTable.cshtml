@model IList<PlantDataMVC.UICore.Models.ViewModels.PlantStock.PlantStockListViewModel>
@using PlantStock = PlantDataMVC.UICore.Models.ViewModels.PlantStock

@{
    PlantStock.PlantStockListViewModel sampleElement = new PlantStock.PlantStockListViewModel();
}

<table class="grid">
    <tr>
        <th>@Html.ColumnHeaderFor(m => sampleElement.SpeciesBinomial)</th>
        <th>@Html.ColumnHeaderFor(m => sampleElement.ProductTypeName)</th>
        <th>@Html.ColumnHeaderFor(m => sampleElement.QuantityInStock)</th>
 @*@       <th></th> *@
        <th></th>
        <th></th>
    </tr>

@{
int count = 0;
foreach (var entry in Model) {
    <tr>
        <td><a asp-controller="PlantStock" asp-action="Show" asp-route-id="@entry.Id">@entry.SpeciesBinomial</a></td>
        <td>@Html.DisplayFor(m => m.ElementAt(count).ProductTypeName)</td>
        <td>@Html.DisplayFor(m => m.ElementAt(count).QuantityInStock)</td>
@*        <td><a asp-controller="PlantStock" asp-action="Edit" asp-route-id="@entry.Id">Edit</a></td> *@
        <td><a asp-controller="PlantStock" asp-action="Details" asp-route-id="@entry.Id">Details</a></td>
        <td><a asp-controller="PlantStock" asp-action="Delete" asp-route-id="@entry.Id">Delete</a></td>
    </tr>
    count++;
}
}
</table>